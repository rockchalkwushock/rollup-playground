{
  "name": "rollup-playground",
  "version": "0.1.0",
  "description": "Getting started with Rollup.",
  "browser": "dist/rollup-playground.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": ["dist", "es", "lib"],
  "repository": "https://github.com/rockchalkwushock/rollup-playground.git",
  "author": "Cody Brunner <rockchalkwushock@icloud.com> (https://rockchalkwushock.github.io/rcws-development/)",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:cjs-umd": "cross-env BABEL_ENV=cjs NODE_ENV=production rollup -c",
    "build:es": "cross-env NODE_ENV=production rollup -c rollup.config.es.js",
    "clean": "rimraf dist es lib package *.tgz",
    "flow": "flow check",
    "lint": "eslint src",
    "lint:fix": "yarn lint -- --fix",
    "start": "yarn run clean && yarn build",
    "sandbox": "npm-run-all --parallel sandbox:*",
    "sandbox:cjs": "babel-node sandbox-cjs.js",
    "sandbox:es": "cross-env BABEL_ENV=sandbox babel-node sandbox-es.js"
  },
  "devDependencies": {
    "babel-cli": "~6.24.1",
    "babel-plugin-external-helpers": "~6.22.0",
    "babel-plugin-syntax-flow": "~6.18.0",
    "babel-plugin-transform-flow-strip-types": "~6.22.0",
    "babel-preset-env": "~1.4.0",
    "babelrc-rollup": "~3.0.0",
    "cross-env": "~4.0.0",
    "eslint": "~3.19.0",
    "eslint-config-equimper": "~1.6.3",
    "eslint-plugin-flowtype": "~2.32.1",
    "flow-bin": "~0.44.2",
    "flow-typed": "~2.1.2",
    "npm-run-all": "~4.0.2",
    "rimraf": "~2.6.1",
    "rollup": "~0.41.6",
    "rollup-plugin-babel": "~2.7.1",
    "rollup-plugin-cleanup": "~1.0.0",
    "rollup-plugin-commonjs": "~8.0.2",
    "rollup-plugin-conditional": "~1.1.1",
    "rollup-plugin-eslint": "~3.0.0",
    "rollup-plugin-filesize": "~1.3.2",
    "rollup-plugin-flow": "~1.1.1",
    "rollup-plugin-gzip": "~1.1.0",
    "rollup-plugin-multidest": "~1.0.1",
    "rollup-plugin-node-resolve": "~3.0.0",
    "rollup-plugin-replace": "~1.1.1",
    "rollup-plugin-uglify": "~1.0.2",
    "rollup-plugin-visualizer": "~0.2.1"
  }
}
